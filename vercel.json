{
  "version": 2,
  "functions": {
    "api/*.php": { "runtime": "vercel-php@0.7.2" }
  },
  "routes": [
    {
      "src": "/secureproxy.php",
      "dest": "/api/secureproxy.php"
    },
    {
      "src": "/api/(.*\\.php)$",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/api/index.php"
    },
    {
      "src": "/tutorial",
      "dest": "/api/tutorial.php"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))$",
      "dest": "/public/$1"
    }
  ]
}
